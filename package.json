{
  "name": "energynet-client",
  "version": "0.0.0",
  "description": "Energynet client made with Redux and React",
  "engines": {
    "npm": ">=3"
  },
  "author": "Alexandr Bladyko",
  "repository": {
    "url": "https://github.com/alexanderbladyko/energynet-client",
    "type": "git"
  },
  "license": "MIT",
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config config/webpack.prod.js",
    "build": "npm rebuild node-sass && npm run clean && npm run build:webpack",
    "clean": "rimraf build",
    "start": "npm rebuild node-sass && node tasks/devServer.js",
    "mocks": "node tasks/mockServer.js",
    "test": "jest",
    "test-coverage": "jest --coverage",
    "docs:api": "mkdir -p build && raml2html docs/api.raml > build/api.html"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.0-beta.3",
    "chai": "^3.5.0",
    "express": "^4.14.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.30.1",
    "image-webpack-loader": "^3.2.0",
    "jest": "^19.0.2",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.3.1",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.6",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.0",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  },
  "dependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/classnames": "^0.0.32",
    "@types/deep-freeze": "^0.0.29",
    "@types/es6-collections": "^0.5.29",
    "@types/es6-promise": "^0.0.32",
    "@types/es6-shim": "^0.31.32",
    "@types/isomorphic-fetch": "^0.0.31",
    "@types/jest": "^19.2.2",
    "@types/mapbox-gl": "^0.29.0",
    "@types/react": "^0.14.41",
    "@types/react-dom": "^0.14.17",
    "@types/react-redux": "^4.4.32",
    "@types/react-scrollbar": "^0.4.6",
    "@types/redux": "^3.6.31",
    "@types/socket.io-client": "^1.4.28",
    "@types/whatwg-fetch": "^0.0.32",
    "autoprefixer": "^6.5.3",
    "bluebird": "^3.4.6",
    "classnames": "^2.2.5",
    "cors": "^2.8.4",
    "css-loader": "^0.26.1",
    "deep-freeze": "^0.0.1",
    "es6-collections": "^0.5.6",
    "es6-promise": "^4.0.5",
    "es6-shim": "^0.35.1",
    "isomorphic-fetch": "^2.2.1",
    "mapbox-gl": "^0.29.0",
    "node-sass": "3.13.0",
    "postcss-easysprites": "^0.1.7",
    "react": "^15.4.0",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-scrollbar": "^0.5.1",
    "redux": "^3.6.0",
    "sass-loader": "^4.0.2",
    "socket.io": "^1.7.2",
    "socket.io-client": "^2.0.2",
    "style-loader": "^0.13.1",
    "stylelint-scss": "^1.3.4",
    "stylelint-webpack-plugin": "^0.4.0",
    "ts-loader": "^1.3.3",
    "typescript": "^2.0.3",
    "whatwg-fetch": "^2.0.1"
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "testRegex": "<rootDir>(test/).*\\.(test|spec)\\.(ts|js)$",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.d.ts"
    ]
  },
  "main": "index.js"
}
